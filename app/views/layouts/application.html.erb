<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= full_title(yield(:title)) %></title>
    <%= stylesheet_link_tag "isotope", "fancybox/jquery.fancybox", "bootstrap", "bootstrap-theme", "style", "default", media: "screen", type:"text/css", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'layouts/navigation' %>
    
    <%= render 'layouts/messages' %>
    <%= yield %>

    <!-- Javascripts -->
    <script src="assets/modernizr-2.6.2-respond-1.1.0.min.js"></script>
    <script src="assets/jquery.js"></script>
    <script src="assets/jquery.easing.1.3.js"></script>
    <script src="assets/bootstrap.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyASm3CwaK9qtcZEWYa-iQwHaGi3gcosAJc&sensor=false"></script>
    <script src="assets/jquery.isotope.min.js"></script>
    <script src="assets/jquery.nicescroll.min.js"></script>
    <script src="assets/fancybox/jquery.fancybox.pack.js"></script>
    <script src="assets/skrollr.min.js"></script>   
    <script src="assets/jquery.scrollTo-1.4.3.1-min.js"></script>
    <script src="assets/jquery.localscroll-1.2.7-min.js"></script>
    <script src="assets/stellar.js"></script>
    <script src="assets/jquery.appear.js"></script>
    <script src="assets/validate.js"></script>
    <script src="assets/main.js"></script>
      

  </body>
</html>
