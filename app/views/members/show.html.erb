<section class="section appear clearfix" id="member-page">
	<div class="container">
		<div class="row mar-bot40">
			<div class="col-md-offset-2 col-md-8 member">
				<div class="row">
					<div class="col-md-4">
						<%= image_tag @member.photo(:med) %>
					</div>
					<div class="col-md-8">
						<h1><%= @member.name %></h1>
						<h4><%= @member.title %></h4>
					</div>
				</div>
				<div class="row">
					
				</div>
			</div>
		</div>
	</div>
</section>