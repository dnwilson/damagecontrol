<section class="panel">
	<div class="container">
		<div class="row mar-bot40">
			<div class="col-md-12 col-sm-12 col-xs-12">
				<% if !@events.empty? %>
					<div class="events-list">
						<% @events.each do |event| %>
							<%= render partial: "event", locals: {event: event} %>
						<% end %>
					</div>
				<% end %>
			</div>
		</div>
	</div>
</section>
